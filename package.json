{
  "name": "flow-template-generator",
  "version": "0.0.1",
  "description": "Flow Template Generator",
  "scripts": {
    "build": "microbundle",
    "lint": "eslint -c .eslintrc.js src",
    "prettify": "prettier --config ./.prettierrc.json --write ./src",
    "test": "jest",
    "check-headers": "sh ./check-headers.sh",
    "prepublishOnly": "npm test && npm run build",
    "publish": "npm publish"
  },
  "source": "src/index.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onflow/flow-template-generator.git"
  },
  "keywords": [
    "flow",
    "cadence",
    "template",
    "generator"
  ],
  "author": "Maksim Daunarovich",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/onflow/flow-template-generator/issues"
  },
  "homepage": "https://github.com/onflow/flow-template-generator#readme",
  "jest": {
    "transform": {
      ".js": "jest-esm-transformer"
    }
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-esm-transformer": "^1.0.0",
    "prettier": "^2.3.0"
  }
}
